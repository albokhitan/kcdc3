var eventLocations = {
	
	"type": "FeatureCollection",

    "features": [
				
		{% for event in events reversed %}
		
        {
            "type": "Feature",
            "geometry": {
                "coordinates": [
                    {{ event.location.get_lng }},
                    {{ event.location.get_lat }}
                ],
                "type": "Point"
            },
            "properties": {
                "name": "{{ event.title | escapejs }}",
				"year": "{{ event.date|date:"Y" }}",
				"session": "{{ event.session.slug }}"
            }
        }{% if not forloop.last %},{% endif %}
		
		{% endfor %}
				
    ]
	
}
